{
  "routes": [
    {
      "method": "DELETE",
      "path": "/indexes",
      "handler": "meilisearch.deleteAllIndexes",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/indexes/:indexUid",
      "handler": "meilisearch.deleteIndex",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/indexes/:indexUid/documents",
      "handler": "meilisearch.addDocuments",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/indexes/:indexUid/update/:updateId",
      "handler": "meilisearch.waitForDocumentsToBeIndexed",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/credentials",
      "handler": "meilisearch.getCredentials",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/credentials",
      "handler": "meilisearch.addCredentials",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/collections",
      "handler": "meilisearch.getCollections",
      "config": {
        "policies": []
      }
    }
    ,
    {
      "method": "POST",
      "path": "/collections",
      "handler": "meilisearch.addCollection",
      "config": {
        "policies": []
      }
    }
  ]
}
