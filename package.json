{
  "name": "strapi-plugin-meilisearch",
  "version": "0.0.0",
  "description": "This is the description of the plugin.",
  "scripts": {
    "style": "eslint --ext .js .",
    "style:fix": "eslint --ext .js . --fix",
    "playground:dev": "yarn --cwd ./playground && yarn --cwd ./playground develop",
    "cypress:install": "node ./node_modules/cypress/bin/cypress install",
    "cypress:open": "cypress open",
    "playground:build": "yarn --cwd ./playground build",
    "playground:start": "yarn --cwd ./playground start",
    "start": "yarn playground:start",
    "cy:run": "cypress run",
    "test": "start-server-and-test start http://localhost:1337 cy:run"
  },
  "strapi": {
    "name": "meilisearch",
    "icon": "plug",
    "description": "Strapi plugin for MeiliSearch"
  },
  "dependencies": {
    "@buffetjs/core": "^3.3.4",
    "meilisearch": "^0.18.1"
  },
  "author": {
    "name": "Charlotte Vermandel",
    "email": "charlotte@meilisearch.com"
  },
  "maintainers": [
    {
      "name": "Charlotte Vermandel",
      "email": "charlotte@meilisearch.com"
    }
  ],
  "engines": {
    "node": ">=10.16.0 <=14.x.x",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "strapi",
    "plugin",
    "meilisearch"
  ],
  "license": "MIT",
  "homepage": "https://github.com/meilisearch/strapi-plugin-meilisearch#readme",
  "devDependencies": {
    "cypress": "^6.6.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "start-server-and-test": "^1.12.0"
  }
